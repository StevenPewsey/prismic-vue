{
	"name": "@prismicio/vue",
	"version": "3.0.0-beta.1",
	"description": "Vue plugin, components, and composables to fetch and present Prismic content",
	"keywords": [
		"typescript",
		"prismic",
		"vue",
		"plugin"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-vue.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"vetur"
	],
	"scripts": {
		"build": "siroc build",
		"dev": "siroc build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"playground:build": "vite build playground",
		"playground:dev": "vite playground",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:beta": "npm run test && standard-version --release-as major --prerelease beta && git push --follow-tags && npm run build && npm publish --tag beta",
		"release:beta:dry": "standard-version --release-as major --prerelease beta --dry-run",
		"release:dry": "standard-version --dry-run",
		"size": "size-limit",
		"test": "npm run lint && npm run unit && npm run build && npm run size",
		"unit": "nyc --reporter=lcovonly --reporter=text --exclude-after-remap=false ava"
	},
	"dependencies": {
		"@prismicio/client": "^6.2.0",
		"@prismicio/helpers": "^2.1.1",
		"@prismicio/types": "^0.1.24",
		"isomorphic-unfetch": "^3.1.0",
		"vue-router": "^4.0.12"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.0.6",
		"@size-limit/preset-small-lib": "^7.0.8",
		"@types/escape-html": "^1.0.1",
		"@types/jsdom-global": "3.0.2",
		"@types/sinon": "10.0.11",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"@vitejs/plugin-vue": "^2.2.0",
		"@vue/compiler-sfc": "^3.2.31",
		"@vue/eslint-config-typescript": "^10.0.0",
		"@vue/test-utils": "2.0.0-rc.17",
		"ava": "^3.15.0",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"eslint-plugin-vue": "^8.4.1",
		"jsdom": "^19.0.0",
		"jsdom-global": "^3.0.2",
		"nyc": "^15.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-jsdoc": "^0.3.30",
		"sinon": "^13.0.1",
		"siroc": "^0.16.0",
		"size-limit": "^7.0.8",
		"standard-version": "^9.3.2",
		"ts-eager": "^2.0.2",
		"typescript": "^4.5.5",
		"vite": "^2.8.2",
		"vue": "^3.2.31"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"engines": {
		"node": ">=12.7.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"vetur": {
		"tags": "vetur/tags.json",
		"attributes": "vetur/attributes.json"
	}
}
